<xsl:stylesheet version="1.0"xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:template match="GlobalNav">		<GlobalNav>			<xsl:apply-templates />		</GlobalNav>	</xsl:template>	<xsl:template match="Item">		<Item  SiteId="{SiteId}" 				SiteTitle="{SiteTitle}"				SiteDescription="{SiteDescription}"			    SiteUrl="{SiteUrl}" 				Position="{Position}"				ParentId="{ParentId}" />	</xsl:template></xsl:stylesheet>